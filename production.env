# 生产环境配置
DJANGO_ENV=production
DEBUG=False

# 数据库配置 - 使用实际生产数据库
DB_NAME=prod_django_db
DB_USER=prod_db_user
DB_PASSWORD=StrongPassword!123
DB_HOST=mysql.prod.internal
DB_PORT=3306

# 安全配置 - 必须重新生成
SECRET_KEY=your-very-secure-secret-key-generated-for-production

# 管理员账号 - 使用更强密码
ADMIN_ACCOUNT=sysadmin
ADMIN_INITIAL_PASSWORD=VeryStrongP@ssw0rd!123

# 跨域设置 - 指定实际前端域名
CORS_ALLOWED_ORIGINS=https://yourdomain.com,https://app.yourdomain.com

# 其他生产环境专用设置
ALLOWED_HOSTS=yourdomain.com,api.yourdomain.com,192.168.1.100